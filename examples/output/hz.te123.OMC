35                 MVP_RP2  = 'Mitral Replacement'
36                 MVP_RP3  = 'Mitral Replacement'
37                 TVP_RP1  = 'Tricuspid Replacement'
38                 TVP_RP2  = 'Tricuspid Replacement'
39                 TVP_RP3  = 'Tricuspid Replacement'
40                 RP1DJUL  = 'First reop Julian date'
