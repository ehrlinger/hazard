46           ;
47         *******************************************************************************;
48         * Censor all patients at valve replacement                                     ;
49           IF TE1=1 AND TE1DJUL=. THEN DELETE;
50           IF TE1=. THEN TE1=0; IF TE2=. THEN TE2=0; IF TE3=. THEN TE3=0;
51           IF AVP_RP3='R' OR MVP_RP3='R' OR TVP_RP3='R' THEN DO;
52             REPL=1; REPLDJUL=RP3DJUL; REPLDATE=REOP3DAT; END;
53           IF AVP_RP2='R' OR MVP_RP2='R' OR TVP_RP2='R' THEN DO;
54             REPL=1; REPLDJUL=RP2DJUL; REPLDATE=REOP2DAT; END;
55           IF AVP_RP1='R' OR MVP_RP1='R' OR TVP_RP1='R' THEN DO;
56             REPL=1; REPLDJUL=RP1DJUL; REPLDATE=REOP1DAT; END;
57           IF REPL=. THEN REPL=0;
58           IF REPL=0 THEN REPLDJUL=FUPDJUL;
59         
60           IF REPL=1 THEN DO ;
61             IF TE1=1 THEN DO; IF TE1DJUL GE REPLDJUL THEN TE1=0; END;
62             IF TE2=1 THEN DO; IF TE2DJUL GE REPLDJUL THEN TE2=0; END;
63             IF TE3=1 THEN DO; IF TE3DJUL GE REPLDJUL THEN TE3=0; END;
64           END;
65         *******************************************************************************;
66         * Prepare a separate observation for each intra-embolic interval               ;
67         * General interval with be that to replacement                                 ;
68           INT_REPL=INT_DEAD;
69           IF REPL=1 THEN INT_REPL=(REPLDJUL-OPDJUL)*12/365.2425;
70         *                                                                              ;
71         * Output an observation from the time of last TE until INT_REPL                ;
72         * This entails setting CENSORED to 1, the interval to INT_REPL, the event      ;
73         * indicator to 0, and the stating time to that of the preceding event or       ;
74         * operaition (if no event):                                                    ;
75         *                                                                              ;
2                                                          The SAS System                           13:11 Thursday, December 1, 1994

76           CENSORED=1; INT_TE=INT_REPL; TE=0; STARTTME=0; NOPREVTE=0;
77           IF TE1=1 THEN DO; STARTTME=12*(TE1DJUL-OPDJUL)/365.2425; NOPREVTE=1; END;
78               /* 2 patients had embolic events in the hospital */
79             IF STUDY='039' THEN STARTTME=(3/24)*12/365.2425;
80             IF STUDY='096' THEN STARTTME=(2/24)*12/365.2425;
81               /* 1 patient died on same day as te */
82             IF STUDY='173' THEN DO; STARTTME=0; TE=1; END;
83           IF TE2=1 THEN DO; STARTTME=12*(TE2DJUL-OPDJUL)/365.2425; NOPREVTE=2; END;
84           IF TE3=1 THEN DO; STARTTME=12*(TE3DJUL-OPDJUL)/365.2425; NOPREVTE=3; END;
85           OUTPUT;
86         *                                                                              ;
87         * Now calculate a inter-event segement for each TE, and output as a separate   ;
88         * observation with starting time the time from t=0 of the immediately prior    ;
89         * event (or ; * the operation) and the ending time the time from t=0 of the    ;
90         * next event or censoring.                                                     ;
91         *                                                                              ;
92         * First event:                                                                 ;
93           IF TE1=1 AND STUDY NE '173' THEN DO;
94             STARTTME=0; INT_TE=12*(TE1DJUL-OPDJUL)/365.2425;
95             /* 2 patients had embolic events in the hospital */
96             IF STUDY='039' THEN INT_TE=(3/24)*12/365.2425;
97             IF STUDY='096' THEN INT_TE=(2/24)*12/365.2425;
98             CENSORED=0; TE=1; NOPREVTE=0;
99             OUTPUT;
100          END;
101        *                                                                              ;
102        * Second event:                                                                ;
103          IF TE2=1 THEN DO;
104            STARTTME=12*(TE1DJUL-OPDJUL)/365.2425;
105            IF STUDY='039' THEN STARTTME=(3/24)*12/365.2425;
106            IF STUDY='096' THEN STARTTME=(2/24)*12/365.2425;
107            INT_TE=12*(TE2DJUL-OPDJUL)/365.2425;
108            CENSORED=0; TE=1; NOPREVTE=1;
109            OUTPUT;
110          END;
111        *                                                                              ;
112        * Third event:                                                                 ;
113          IF TE3=1 THEN DO;
114            STARTTME=12*(TE2DJUL-OPDJUL)/365.2425;
115            INT_TE=12*(TE3DJUL-OPDJUL)/365.2425;
116            CENSORED=0; TE=1; NOPREVTE=2;
117            OUTPUT;
118          END;
119          KEEP STUDY TE1 TE2 TE3 INT_TE STARTTME CENSORED TE NOPREVTE;
