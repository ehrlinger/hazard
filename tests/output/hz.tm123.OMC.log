538     DATA PRIMISOL; INFILE INOMC MISSOVER;
539     INPUT STUDY $ 1-10 TE1 12 TE2 14 TE3 16 TE1DJUL 18-25 TE2DJUL 27-33 TE3DJUL
540           35-41 AVP_RP1 $ 43 AVP_RP2 $ 45 AVP_RP3 $ 47 MVP_RP1 $ 49 MVP_RP2 $ 51
541           MVP_RP3 $ 53 TVP_RP1 $ 55 TVP_RP2 $ 57 TVP_RP3 $ 59 RP1DJUL 61-67
542           RP2DJUL 69-75 RP3DJUL 77-83 REOP1DAT $ 85-92 REOP2DAT $ 94-101 REOP3DAT
543           $ 103-110 INT_DEAD 111-120 5 OPDJUL 122-128 DEAD 130 FUPDJUL 132-138
544           TE1GRADE 140 TE2GRADE 142 TE3GRADE 144;
545
546     LABEL
547           STUDY    = 'Study number'
548           TE1      = 'First TE'
549           TE2      = 'Second TE'
550           TE3      = 'Third TE'
551           TE1DJUL  = 'First TE Julian date'
552           TE2DJUL  = 'Second TE Julian date'
553           TE3DJUL  = 'Third TE Julian date'
554           AVP_RP1  = 'Aortc Replacement'
555           AVP_RP2  = 'Aortc Replacement'
556           AVP_RP3  = 'Aortc Replacement'
557           MVP_RP1  = 'Mitral Replacement'
558           MVP_RP2  = 'Mitral Replacement'
559           MVP_RP3  = 'Mitral Replacement'
560           TVP_RP1  = 'Tricuspid Replacement'
561           TVP_RP2  = 'Tricuspid Replacement'
562           TVP_RP3  = 'Tricuspid Replacement'
563           RP1DJUL  = 'First reop Julian date'
564           RP2DJUL  = 'Second reop Julian date'
565           RP3DJUL  = 'Thrid reop Julian date'
566           DEAD     = 'Death'
567           INT_DEAD = 'Followup interval (months) after repair'
568           FUPDJUL  = 'Followup Julian date'
569     ;
570   *******************************************************************************;
571   * Prepare a separate observation for each intra-embolic interval               ;
572     IF TE1=1 AND TE1DJUL=. THEN DELETE;
573     IF TE1=. THEN TE1=0; IF TE2=. THEN TE2=0; IF TE3=. THEN TE3=0;
574     IF AVP_RP3='R' OR MVP_RP3='R' OR TVP_RP3='R' THEN DO;
575       REPL=1; REPLDJUL=RP3DJUL; REPLDATE=REOP3DAT; END;
576     IF AVP_RP2='R' OR MVP_RP2='R' OR TVP_RP2='R' THEN DO;
577       REPL=1; REPLDJUL=RP2DJUL; REPLDATE=REOP2DAT; END;
578     IF AVP_RP1='R' OR MVP_RP1='R' OR TVP_RP1='R' THEN DO;
579       REPL=1; REPLDJUL=RP1DJUL; REPLDATE=REOP1DAT; END;
580     IF REPL=. THEN REPL=0;
581     IF REPL=0 THEN REPLDJUL=FUPDJUL;
582
583     IF REPL=1 THEN DO ;
584       IF TE1=1 THEN DO; IF TE1DJUL GE REPLDJUL THEN TE1=0; END;
585       IF TE2=1 THEN DO; IF TE2DJUL GE REPLDJUL THEN TE2=0; END;
586       IF TE3=1 THEN DO; IF TE3DJUL GE REPLDJUL THEN TE3=0; END;
587     END;
588
589     INT_REPL=INT_DEAD;
590     IF REPL=1 THEN INT_REPL=(REPLDJUL-OPDJUL)*12/365.2425;
591
592     CENSORED=1; INT_TE=INT_REPL; TE=0; STARTTME=0; MORBID=0;
593     IF TE1=1 THEN STARTTME=12*(TE1DJUL-OPDJUL)/365.2425;
594         /* 2 patients had embolic events in the hospital */
595       IF STUDY='039' THEN STARTTME=(3/24)*12/365.2425;
596       IF STUDY='096' THEN STARTTME=(2/24)*12/365.2425;
597         /* 1 patient died on same day as te */
598    /*    IF STUDY='173' THEN DO; STARTTME=0; TE=1; END; */
599   * The previous statement is an effort to remove a zero censored interval, but
600   * it is placed incorrectly and produces and error (TE is remodulated at wrong
601   * level). ;
602     IF TE2=1 THEN STARTTME=12*(TE2DJUL-OPDJUL)/365.2425;
603     IF TE3=1 THEN STARTTME=12*(TE3DJUL-OPDJUL)/365.2425;
604     OUTPUT;
605
606     IF TE1=1 /* AND STUDY NE '173' */ THEN DO;
607   * See previous comment about study # 173;
608       STARTTME=0; INT_TE=12*(TE1DJUL-OPDJUL)/365.2425;
609       /* 2 patients had embolic events in the hospital */
610       IF STUDY='039' THEN INT_TE=(3/24)*12/365.2425;
611       IF STUDY='096' THEN INT_TE=(2/24)*12/365.2425;
612       CENSORED=0; TE=1;
613       MORBID=TE1GRADE; IF TE1GRADE=. THEN MORBID=1.70;
614       OUTPUT;
615     END;
616
617     IF TE2=1 THEN DO;
618       STARTTME=12*(TE1DJUL-OPDJUL)/365.2425;
619       IF STUDY='039' THEN STARTTME=(3/24)*12/365.2425;
620       IF STUDY='096' THEN STARTTME=(2/24)*12/365.2425;
621       INT_TE=12*(TE2DJUL-OPDJUL)/365.2425;
622       CENSORED=0; TE=1;
623       MORBID=TE2GRADE; IF TE2GRADE=. THEN MORBID=2.17;
624       OUTPUT;
625     END;
626
627     IF TE3=1 THEN DO;
628       STARTTME=12*(TE2DJUL-OPDJUL)/365.2425;
629       INT_TE=12*(TE3DJUL-OPDJUL)/365.2425;
630       CENSORED=0; TE=1;
631       MORBID=TE3GRADE; IF TE3GRADE=. THEN MORBID=4.00;
632       OUTPUT;
633     END;
634     KEEP STUDY TE1 TE2 TE3 INT_TE STARTTME CENSORED TE MORBID;
